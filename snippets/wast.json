{
    "export": {
        "prefix": "export",
        "body": "(export \"${name}\" $${name})"
    },
    "import": {
        "prefix": "import",
        "body": "(import $${name} \"${from}\" \"${name}\" (param ${type}))"
    },
    "if": {
        "prefix": "if",
        "body": "(if ($1)\n\t$2\n)"
    },
    "loop": {
        "prefix": "loop",
        "body": "(loop $exit $cont\n\t$1\n)"
    },
    "func": {
        "prefix": "func",
        "body": "(func $${name} (param $1) (result $2)\n\t$3\n)"
    }
}