{
    "export": {
        "prefix": "export",
        "body": "(export \"${1:name}\" $${1:name})$2"
    },
    "import": {
        "prefix": "import",
        "body": "(import $${1:name} \"${2:from}\" \"${3:name}\" (param ${4:type}))$5"
    },
    "loop": {
        "prefix": "loop",
        "body": "(loop $exit $cont\n\t$1\n)"
    },
    "func": {
        "prefix": "func",
        "body": "(func $${1:name} (param $2) (result $3)\n\t$4\n)"
    }
}